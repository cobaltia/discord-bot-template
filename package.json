{
	"name": "discord-bot-template",
	"version": "0.1.0",
	"main": "./dist/Bot.js",
	"scripts": {
		"clean": "node scrips/clean.mjs",
		"build": "tsc -b src",
		"dev": "yarn build && yarn start:dev",
		"prettier": "prettier --write .",
		"start:dev": "node --enable-source-maps dist/Cobalt.js | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss",
		"test": "jest",
		"test:ci": "yarn test --verbose --noStackTrace",
		"test:prettier": "prettier --check .",
		"test:typescipt": "tsc -b src"
	},
	"repository": "https://github.com/cobaltia/discord-bot-template.git",
	"author": "Isidro Casiano <personal@isidrocasiano.com>",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.21",
		"jest": "^27.5.1",
		"pino-pretty": "^7.5.3",
		"prettier": "^2.5.1",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2"
	},
	"dependencies": {
		"discord.js": "^13.6.0",
		"pino": "^7.8.0"
	}
}
